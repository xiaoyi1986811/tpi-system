":1,"uuid":"e8561e017735228dbc9cff1b0cd17979"},{"date":"2025-04-21 18:44:43","docid":0,"highlighter":true,"id":21,"imgfile":"17e039229044b74e7d0b0b14fdd01a4c.png","linecolor":"ff60bb46","linestyle":1,"linewidth":13,"page":327,"rect":[124.12499630078685,50.24999850243334,339.3749898858371,49.124998535960955,26.62499920651319,64.12499808892613,335.99998998641996,65.62499804422265,29.624999117106224,80.62499759718783,83.24999751895673,81.37499757483609],"type":1,"uuid":"794d4d014f3561e74514769020339bb9"}]}