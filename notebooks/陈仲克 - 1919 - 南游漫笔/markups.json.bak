227baa76690be8a22bffa"},{"date":"2025-04-20 00:58:33","docid":0,"group":[{"date":"2025-04-20 00:58:33","docid":0,"highlighter":true,"id":38,"linecolor":"ff60bb46","linestyle":1,"linewidth":13,"page":6,"rect":[308.4999908059838,60.99999818205839,306.9999908506873,499.99998509883926,283.49999155104183,62.49999813735491,287.49999143183254,218.49999348819276],"type":1,"uuid":"b756293f641bc2d26004f73b064766b9"}],"highlighter":true,"id":37,"imgfile":"704abdd8801539bdbd4c1289b4750ee5.png","linecolor":"ff60bb46","linestyle":1,"linewidth":13,"page":5,"rect":[8.499999746680267,66.49999801814562,17.99999946355821,496.4999852031474],"type":1,"uuid":"037aaca418c539fa65248937bad6e64f"},{"date":"2025-04-20 14:20:33","docid":0,"highlighter":true,"id":41,"imgfile":"0f9475abfc96927999b0d9c460847137.png","linecolor":"ff60bb46","linestyle":1,"linewidth":13,"page":1,"rect":[272.4999918788674,233.99999302625676,274.99999180436157,252.49999247491382],"type":1,"uuid":"c667493f4ebc330eccfab7606071675a"}]}