,"rect":[133.13252615282346,723.2680507340889,130.57228526526916,1127.7861109676678],"type":1,"uuid":"10c55c60502d54ebbb52491b8706d773"},{"date":"2025-03-25 11:26:23","docid":0,"id":36,"imgfile":"27c8d09b6d2954571103595bcd364063.png","linecolor":"ffaa0000","linestyle":1,"linewidth":10,"page":1868,"rect":[721.9879302903117,679.7439556456659,727.1084120654203,1098.3433407607934,679.7439556456659,515.8885388421909,682.3041965332202,887.1234675375639],"type":1,"uuid":"572897e201086dff214d415ad803579b"},{"date":"2025-03-25 11:27:59","docid":0,"id":38,"imgfile":"ccfe0b70f0c10187f80da9321afb444b.png","linecolor":"ffaa0000","linestyle":1,"linewidth":10,"page":1869,"rect":[716.8674485152031,291.86746118118987,714.3072076276488,586.295163249934],"type":1,"uuid":"df0792cd3c9228643887e51f6e4a41a9"},{"date":"2025-03-25 11:29:04","docid":0,"id":40,"imgfile":"80d30cdfb5e30bd2232c0e95b2dae0cf.png","linecolor":"ffaa0000","linestyle":1,"linewidth":10,"page":1869,"rect":[585.0150428061569,651.5813058825686,601.6566085752598,1136.7469540741079],"type":1,"uuid":"5bd6e577971fcc888ee299e30c975824"},{"date":"2025-03-25 11:29:10","docid":0,"id":41,"imgfile":"c8fd905935dc1c8a4e59aaa9981d7c86.png","linecolor":"ffaa0000","linestyle":1,"linewidth":10,"page":1869,"rect":[532.5301046112938,291.86746118118987,542.7710681615109,659.2620285452315],"type":1,"uuid":"9c6b1ac3e2bf1a17360aa6a5b647c7b1"}]}